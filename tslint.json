{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "max-classes-per-file": [
      true,
      1
    ],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "lowercase-last"
      }
    ],
    "typedef": [
      true,
      "call-signature",
      "parameter"
    ]
  }
}